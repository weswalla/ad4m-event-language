{
  "name": "junto-ad4m-languages",
  "version": "1.0.0",
  "description": "AD4M Language implementation for Junto applications",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-icons": "rollup -c rollup.config.icons.js",
    "build": "CARGO_TARGET_DIR=target cargo build --release --target wasm32-unknown-unknown && hc dna pack workdir && npm i && npm run build-icons && tsc && rollup -c",
    "build-dna": "nix-shell --run ./build.sh",
    "build-nix": "npm run build-dna && tsc && rollup -c",
    "dev": "tsc && rollup -c -w"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@perspect3vism/ad4m": "0.1.16",
    "@perspect3vism/rollup-plugin-dna": "^0.0.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^4.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/node": "^14.14.6",
    "blake2b": "^2.1.3",
    "node-sass": "^4.14.1",
    "rollup": "^2.3.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.2.4",
    "xmlhttprequest": "^1.8.0"
  },
  "dependencies": {
    "@jaames/iro": "^5.3.1",
    "@simonwep/pickr": "^1.8.0",
    "autoprefixer": "^10.1.0",
    "postcss": "^8.2.1"
  }
}
